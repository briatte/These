arxivId = {1504.07976},
author = {Erlebach, Thomas and Hoffmann, Michael and Kammer, Frank},
eprint = {1504.07976},
month = {apr},
title = {{On Temporal Graph Exploration}},
url = {http://arxiv.org/abs/1504.07976},
year = {2015}
