isbn = {9781605585062},
keywords = {densification,diameter,social networks,sparsification.},
month = {may},
pages = {427},
publisher = {ACM Press},
title = {{Affiliation networks}},
url = {http://portal.acm.org/citation.cfm?doid=1536414.1536474 http://dl.acm.org/citation.cfm?id=1536414.1536474},
year = {2009}
}
