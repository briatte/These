file = {:home/gaumont/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Sch{\"{a}}rrer et al. - 2015 - Evaluation of farm-level parameters derived from animal movements for use in risk-based surveillance programme.pdf:pdf},
