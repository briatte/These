journal = {PloS one},
keywords = {Communicable Disease Control,Communicable Diseases,Communicable Diseases: therapy,Computer Simulation,Disease Outbreaks,Humans,Immunization,Models,Social Support,Theoretical},
month = {jan},
number = {5},
pages = {e36439},
pmid = {22586472},
title = {{Exploiting temporal network structures of human interaction to effectively immunize populations.}},
